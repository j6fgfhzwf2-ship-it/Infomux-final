{
  "name": "infomux-final-ia-infomux",
  "version": "2.0.0",
  "description": "Projet Electron v2 avec IA et animations interactives",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "package:mac": "electron-packager . InfomuxFinal --platform=darwin --arch=x64 --out=dist --overwrite --icon=assets/icon.icns --app-version=2.0.0",
    "package:win": "electron-packager . InfomuxFinal --platform=win32 --arch=x64 --out=dist --overwrite --icon=assets/icon.ico --app-version=2.0.0",
    "package:linux": "electron-packager . InfomuxFinal --platform=linux --arch=x64 --out=dist --overwrite --app-version=2.0.0"
  },
  "keywords": ["electron", "animations", "IA", "js", "html", "css"],
  "author": "Dev : milaslevrai | Dark : darkolevrai",
  "license": "MIT",
  "devDependencies": {
    "electron": "^26.2.0",
    "electron-packager": "^17.1.0",
    "dotenv": "^16.3.1"
  },
  "dependencies": {}
}
